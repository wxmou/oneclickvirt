auth:
    enable-email: false
    enable-public-registration: true
    enable-qq: false
    enable-telegram: false
captcha:
    enabled: false
    expire-time: 5
    height: 40
    length: 4
    width: 120
cdn:
    base-endpoint: https://cdn.spiritlhl.net/
    endpoints:
        - https://cdn0.spiritlhl.top/
        - http://cdn3.spiritlhl.net/
        - http://cdn1.spiritlhl.net/
        - http://cdn2.spiritlhl.net/
cors:
    mode: allow-all
    whitelist:
        - http://localhost:8080
        - http://127.0.0.1:8080
invite-code:
    enabled: false
    required: false
jwt:
    buffer-time: 1d
    expires-time: 7d
    issuer: oneclickvirt
    signing-key: ""
mysql:
    auto-create: true
    config: charset=utf8mb4&parseTime=True&loc=Local
    db-name: oneclickvirt
    engine: InnoDB
    log-mode: error
    log-zap: "false"
    max-idle-conns: "10"
    max-lifetime: "3600"
    max-open-conns: "100"
    password: ""
    path: 127.0.0.1
    port: "3306"
    prefix: ""
    singular: "false"
    username: root
quota:
    default-level: 1
    level-limits:
        "1":
            max-instances: 6
            max-resources:
                bandwidth: 1000
                cpu: 6
                disk: 66024
                memory: 6350
            max-traffic: 1.024e+06
        "2":
            max-instances: 3
            max-resources:
                bandwidth: 200
                cpu: 2
                disk: 20480
                memory: 1024
            max-traffic: 204800
        "3":
            max-instances: 5
            max-resources:
                bandwidth: 500
                cpu: 4
                disk: 40960
                memory: 2048
            max-traffic: 307200
        "4":
            max-instances: 10
            max-resources:
                bandwidth: 1000
                cpu: 8
                disk: 81920
                memory: 4096
            max-traffic: 409600
        "5":
            max-instances: 20
            max-resources:
                bandwidth: 2000
                cpu: 16
                disk: 163840
                memory: 8192
            max-traffic: 512000
redis:
    addr: ""
    db: 0
    password: ""
system:
    addr: 8888
    db-type: mysql
    env: development
    frontend-url: ""
    iplimit-count: 100
    iplimit-time: 3600
    oauth2-state-token-minutes: 15
    oss-type: local
    provider-inactive-hours: 72
    use-multipoint: false
    use-redis: false
task:
    delete-retry-count: 3
    delete-retry-delay: 2
upload:
    max-avatar-size: 5242880
other:
    default-language: en-US
    max-avatar-size: 5
zap:
    compress-logs: true
    director: storage/logs
    encode-level: LowercaseLevelEncoder
    format: console
    level: debug
    log-in-console: false
    max-array-elements: 5
    max-backups: 15
    max-file-size: 5
    max-log-length: 2000
    max-string-length: 1000
    prefix: '[oneclickvirt]'
    retention-day: 7
    show-line: true
    stacktrace-key: stacktrace
max_avatar_size: 2
default_language:
signing_key_v1:
