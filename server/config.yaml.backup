auth:
    email-password: ""
    email-smtp-host: ""
    email-smtp-port: "3306"
    email-username: root
    enable-email: false
    enable-oauth2: false
    enable-public-registration: false
    enable-qq: false
    enable-telegram: false
    qq-app-id: ""
    qq-app-key: ""
    telegram-bot-token: ""
captcha:
    enabled: false
    expire-time: 5
    height: 40
    length: 4
    width: 120
cdn:
    base-endpoint: https://cdn.spiritlhl.net/
    endpoints:
        - https://cdn0.spiritlhl.top/
        - http://cdn3.spiritlhl.net/
        - http://cdn1.spiritlhl.net/
        - http://cdn2.spiritlhl.net/
cors:
    mode: allow-all
    whitelist:
        - http://localhost:8080
        - http://127.0.0.1:8080
invite-code:
    enabled: true
    required: false
jwt:
    buffer-time: 1d
    expires-time: 7d
    issuer: oneclickvirt
    signing-key: ""
mysql:
    auto-create: true
    config: charset=utf8mb4&parseTime=True&loc=Local&time_zone=%27%2B08%3A00%27
    db-name: oneclickvirt
    engine: InnoDB
    log-mode: error
    log-zap: "false"
    max-idle-conns: "10"
    max-lifetime: "3600"
    max-open-conns: "100"
    password: ""
    path: 127.0.0.1
    port: "3306"
    prefix: ""
    singular: "false"
    username: root
quota:
    instance-type-permissions:
        min-level-for-container: 1
        min-level-for-delete-container: 1
        min-level-for-delete-vm: 2
        min-level-for-reset-container: 1
        min-level-for-reset-vm: 2
        min-level-for-vm: 3
redis:
    addr: ""
    db: 0
    password: ""
system:
    addr: 8888
    db-type: mysql
    env: development
    frontend-url: ""
    iplimit-count: 100
    iplimit-time: 3600
    oauth2-state-token-minutes: 15
    oss-type: local
    provider-inactive-hours: 72
    use-multipoint: false
    use-redis: false
task:
    delete-retry-count: 3
    delete-retry-delay: 2
upload:
    max-avatar-size: 5242880
other:
    default-language: en-US
    max-avatar-size: 5
zap:
    director: storage/logs
    encode-level: LowercaseLevelEncoder
    format: console
    level: debug
    log-in-console: false
    max-array-elements: 5
    max-backups: 15
    max-file-size: 5
    max-log-length: 2000
    max-string-length: 1000
    prefix: '[oneclickvirt]'
    retention-day: 7
    show-line: true
    stacktrace-key: stacktrace
max_avatar_size: 2
default_language:
